{
  "root": "packages/presentation/apollo",
  "sourceRoot": "packages/presentation/apollo/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/presentation/apollo",
        "main": "packages/presentation/apollo/src/main.ts",
        "tsConfig": "packages/presentation/apollo/tsconfig.app.json",
        "assets": ["packages/presentation/apollo/src/assets"]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "packages/presentation/apollo/src/environments/environment.ts",
              "with": "packages/presentation/apollo/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "presentation-apollo:build",
        "host": "0.0.0.0"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/presentation/apollo/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/presentation/apollo"],
      "options": {
        "jestConfig": "packages/presentation/apollo/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["presentation"]
}
